{
  "compilerOptions": {
    "typeRoots": [
      "node_modules/@types"
    ],
    "target": "ES2021",
    "module": "CommonJS",
    "moduleResolution": "node",
    "removeComments": true,
    "sourceMap": false,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "incremental": true,
    "tsBuildInfoFile": "../dist/tokens/tsbuildinfo",
    "types": [
      "node"
    ]
  },
  "include": [
    "actions",
    "bin",
    "commands",
    "enums",
    "lib",
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
